<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.crazy.business.dao.VisitDao">

    <insert id="insert">
        insert into visit_info
        (
        ip,
        time
        )
        values
        (
        #{visitInfo.ip},
        #{visitInfo.time}
        )
    </insert>

</mapper>